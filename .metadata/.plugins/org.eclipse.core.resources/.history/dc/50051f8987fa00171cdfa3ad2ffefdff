package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
		int[] n = { 45, 81, 9, 72, 4, 6};
		
		for(int i=0;i<n.length-1;i++){
			for(int j=1;j<n.length;j++){
				if(n[i]<n[j]){
					int tmp = n[i];
					n[i] = n[j];
					n[j]=tmp;
				}
			}
			
			System.out.println(n);
		}
//		System.out.println(n[0]);
	}
}
