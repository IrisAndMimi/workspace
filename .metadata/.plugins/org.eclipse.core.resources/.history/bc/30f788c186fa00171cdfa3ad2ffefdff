package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
		int[] n = { 45, 81, 9, 72, 4, 6};
		for(int i=0;i<n.length-1;i++){
			if(n[i]<n[i+1]){
				int tmp = n[i];
				n[i] = n[i+1];
				n[i+1]=tmp;
			}
//			System.out.println(n[i]);
		}
//		System.out.println(n[0]);
	}
}
